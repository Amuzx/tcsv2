<f:view xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:p="http://primefaces.org/ui"
	xmlns:c="http://java.sun.com/jsp/jstl/core">
	<ui:composition template="../templates/basicPage.xhtml">
		<ui:param name="menuIndex" value="6" />
		<ui:define name="content">
	
			<p:tabView id="adminTab" scrollable="true" styleClass="tabs">
				<c:forEach items="#{adminTabView.adminTabList}" var="tab">
					<p:tab title="#{tab.name}">
						<ui:include src="../#{tab.url}" />
					</p:tab>
				</c:forEach>
			</p:tabView>
		</ui:define>
	</ui:composition>
</f:view>