	font-weight: 700
}

.content {
	position: absolute;
	overflow: hidden;
	top: 60px;
	bottom: 20px;
	left: 0;
	right: 0
}

.footer {
	position: absolute;
	height: 20px;
	line-height: 20px;
	color: #fff;
	background: #000;
	bottom: 0;
	left: 0;
	right: 0;
	text-align: center;
	font-size: 12px
}

.masterPane {
	position: absolute;
	overflow: auto;
	top: 0;
	left: 0;
	width: 130px;
	height: 100%;
	background: #e8e8e8
}

.detailPane {
	position: absolute;
	top: 0;
	left: 130px;
	right: 0;
	height: 100%;
	overflow: hidden
}

.page {
	position: absolute;
	width: 100%;
	height: 100%;
	overflow: auto;
	background: #fff
}

.container {
	padding: 12px;
	min-width: 1000px
}

.field-closeable {
	position: relative
}

.field-closeable .btn-close {
	position: absolute;
	top: 0;
	right: 0;
	background: #E4E4E5
}

