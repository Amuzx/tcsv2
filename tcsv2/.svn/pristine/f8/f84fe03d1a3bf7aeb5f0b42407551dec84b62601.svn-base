}

.topbar {
	position: relative;
	height: 60px;
	color: #fff;
	background: #00405B;
	z-index: 1000
}

.logo {
	width: 150px;
	text-align: center;
	height: 60px;
	line-height: 60px;
	font-size: 24px;
	font-weight: 700
}

.tools {
	position: absolute;
	top: 0;
	right: 0;
	background: #00405B
}

.tools li {
	float: left;
	border-left: 1px solid #33667C;
	height: 60px
}

.tools .item-title {
	display: block;
	text-align: center;
	font-size: 12px;
	color: #FFF
}

.tools .item {
	display: block;
	position: relative;
	padding: 0 15px;
	height: 60px;
	text-align: center;
	font-size: 20px;
	color: #FFF;
	padding: 0 15px
}

.tools .sub-item-panel {
	display: none;
	position: absolute;
	top: 60px;
	right: -2px;
	color: #33667C;
	border: 1px solid #33667C;
	border-top: 0;
	width: 400px;
	text-align: left;
	background: #fff;
	padding: 4px
}

.tools .item:HOVER .sub-item-panel {
	display: block
}

.tools .sub-item-panel .sub-item {
	display: block;
	position: relative;
	font-size: 12px;
	border-bottom: 1px solid #E4E4E5;
	cursor: default
}

.tools .sub-item-panel .sub-item.last {
	border-bottom: 0
}

.tools .sub-item-panel .sub-item>.label {
	position: absolute;
	left: 0;
	top: 0;
	width: 120px;
	padding: 10px 4px;
	font-weight: 700
}

.tools .sub-item-panel .sub-item>.value {
	display: block;
	margin-left: 128px;
	padding: 10px 4px;
	min-height: 14px
}

.tools li:HOVER {
	background: #33667C
}

.tools .item [class^=icon-], [class*=" icon-"] {
	line-height: 40px
}

.tools .item .text {
	display: inline-block;
	font-size: 14px;
	height: 60px;
	line-height: 60px
}

.tools .item .user {
	padding-right: 20px;
	position: relative
}

.tools .item .user:AFTER {
	content: "";
	display: block;
	border: 6px solid transparent;
	border-top-color: #fff;
	position: absolute;
	top: 22px;
	right: 0
}

