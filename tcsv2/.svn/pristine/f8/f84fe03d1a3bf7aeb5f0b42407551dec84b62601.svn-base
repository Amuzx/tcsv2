.tabs.ui-tabs .ui-tabs-nav {
	background: 0;
	border-bottom: 2px solid #33667C;
	padding: 0 0 0 8px
}

.tabs.ui-tabs .ui-tabs-nav li.ui-state-default {
	margin: 0 8px;
	background: #E4E4E5;
	border: 0;
	padding: 0 10px;
	height: 30px;
	line-height: 30px;
	cursor: pointer;
	top: 0
}

.tabs.ui-tabs .ui-tabs-nav li.ui-state-default a {
	color: #222;
	font-size: 14px;
	font-weight: 700;
	padding: 0
}

.tabs.ui-tabs .ui-tabs-nav li.ui-state-default:BEFORE {
	content: "";
	display: block;
	position: absolute;
	width: 0;
	height: 0;
	top: 0;
	left: -10px;
	border-top: 15px solid transparent;
	border-left: 5px solid transparent;
	border-right: 5px solid #E4E4E5;
	border-bottom: 15px solid #E4E4E5
}

.tabs.ui-tabs .ui-tabs-nav li.ui-state-default:AFTER {
	content: "";
	display: block;
	position: absolute;
	width: 0;
	height: 0;
	top: 0;
	right: -10px;
	border-top: 15px solid transparent;
	border-right: 5px solid transparent;
	border-left: 5px solid #E4E4E5;
	border-bottom: 15px solid #E4E4E5
}

.tabs.ui-tabs .ui-tabs-nav li.ui-state-hover {
	background: #668C9D;
	color: #fff;
	z-index: 81
}

.tabs.ui-tabs .ui-tabs-nav li.ui-state-hover a {
	color: #fff
}

.tabs.ui-tabs .ui-tabs-nav li.ui-state-hover:BEFORE {
	border-right-color: #668C9D;
	border-bottom-color: #668C9D
}

.tabs.ui-tabs .ui-tabs-nav li.ui-state-hover:AFTER {
	border-left-color: #668C9D;
	border-bottom-color: #668C9D
}

.tabs.ui-tabs .ui-tabs-nav li.ui-state-active {
	background: #33667C;
	z-index: 80;
	cursor: default
}

.tabs.ui-tabs .ui-tabs-nav li.ui-state-active a {
	color: #fff;
	cursor: default
}

.tabs.ui-tabs .ui-tabs-nav li.ui-state-active:BEFORE {
	border-right-color: #33667C;
	border-bottom-color: #33667C
}

.tabs.ui-tabs .ui-tabs-nav li.ui-state-active:AFTER {
	border-left-color: #33667C;
	border-bottom-color: #33667C
}

