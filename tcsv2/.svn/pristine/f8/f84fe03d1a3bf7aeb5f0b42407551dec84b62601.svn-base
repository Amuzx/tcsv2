	border-bottom: 1px solid #fff;
	text-align: center;
	font-size: 14px;
	font-weight: 700;
	color: #4C5356;
	cursor: pointer;
	padding: 12px 4px 8px
}

.menu>.item>a>i {
	font-size: 28px
}

.menu>.item>a>span {
	display: block;
	text-align: center;
	font-size: 12px;
	font-weight: 700;
	color: #4C5356;
	cursor: pointer;
	margin-top: 6px
}

.menu>.item:HOVER>a {
	background: #668C9D;
	color: #fff
}

.menu>.item:HOVER>a>span {
	color: #fff
}

.menu>.item.selected>a {
	background: #33667C;
	color: #fff
}

